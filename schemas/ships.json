{
  "$id": "http://example.com/example.json",
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "Ship schema",
  "description": "A schema for ships used in STFC.",
  "required": [
    "ship_name",
    "rarity",
    "grade",
    "ship_class",
    "faction",
    "img_url",
    "ship_ability",
    "blueprints",
    "requirements",
    "scrap_info",
    "tiers"
  ],
  "type": "object",
  "additionalProperties": true,
  "default": {},
  "examples": [
    {
      "blueprints": {
        "cost": 200,
        "quantity": 100
      },
      "faction": "federation",
      "grade": 3,
      "img_url": "url",
      "rarity": "uncommon",
      "requirements": [
        {
          "name": "shipyard",
          "level": 28
        }
      ],
      "scrap_info": {
        "required_scrapyard_level": 42,
        "scrapable": true
      },
      "ship_ability": {
        "title": "Protector",
        "description": "When the Station..."
      },
      "ship_class": "interceptor",
      "ship_name": "USS Saladin",
      "tiers": [
        {
          "build_time": 1468800,
          "components": [
            {
              "name": "Phaser Bank",
              "additional_info": {
                "cargo_info": {
                  "increased_cargo": 1960,
                  "increased_protected_cargo": 10
                },
                "firing_pattern": {
                  "pattern": [
                    0,
                    0,
                    1,
                    0,
                    0,
                    1,
                    0,
                    0,
                    1
                  ],
                  "weapon_type": "kinetic"
                },
                "warp_info": {
                  "impulse_speed": 120,
                  "warp_distance": 27,
                  "warp_speed": 2.4
                }
              },
              "build_cost": {
                "materials": [
                  {
                    "type": "gas",
                    "grade": 3,
                    "rarity": "common",
                    "value": 275
                  }
                ],
                "others": [
                  {
                    "type": "charged nanoprobes",
                    "value": 500
                  }
                ],
                "resources": [
                  {
                    "type": "tritanium",
                    "value": 1300000
                  }
                ]
              },
              "img_url": "url",
              "repair_cost": [
                {
                  "type": "tritanium",
                  "value": 46437
                }
              ]
            }
          ],
          "levels": [
            {
              "ability_bonus": 16.0,
              "additional_officer_slot": true,
              "level": 2,
              "scrap_details": {
                "rewards": [
                  {
                    "type": "gas",
                    "grade": 3,
                    "rarity": "common",
                    "value": 40000
                  }
                ],
                "scrap_time": 895620
              },
              "ship_xp": 800
            }
          ],
          "repair_time": 30000,
          "tier": 1,
          "tier_locked_by_ops": 30
        }
      ]
    }
  ],
  "properties": {
    "blueprints": {
      "$id": "#/properties/blueprints",
      "title": "The blueprints schema",
      "description": "Information about the blueprints for the ship.",
      "required": [
        "quantity",
        "cost"
      ],
      "type": "object",
      "additionalProperties": true,
      "default": {},
      "properties": {
        "cost": {
          "$id": "#/properties/blueprints/properties/cost",
          "title": "The cost schema",
          "description": "Cost of a single blueprint for the ship.",
          "type": "integer",
          "default": 0,
          "examples": [
            200
          ]
        },
        "quantity": {
          "$id": "#/properties/blueprints/properties/quantity",
          "title": "The quantity schema",
          "description": "Number of blueprints needed to build the ship.",
          "type": "integer",
          "default": 0,
          "examples": [
            100
          ]
        }
      }
    },
    "faction": {
      "$id": "#/properties/faction",
      "title": "The faction schema",
      "description": "The faction where the ship belongs to.",
      "type": "string",
      "default": "",
      "enum": [
        "neutral",
        "federation",
        "romulan",
        "klingon",
        "augment",
        "rogue"
      ],
      "examples": [
        "federation"
      ]
    },
    "grade": {
      "$id": "#/properties/grade",
      "title": "The grade schema",
      "description": "The grade of the ship with stars",
      "type": "integer",
      "default": 1,
      "enum": [
        1,
        2,
        3,
        4,
        5
      ],
      "examples": [
        1
      ]
    },
    "img_url": {
      "$id": "#/properties/img_url",
      "title": "The img_url schema",
      "description": "The URL of the ships image",
      "type": "string",
      "default": "",
      "examples": [
        "url"
      ]
    },
    "rarity": {
      "$id": "#/properties/rarity",
      "title": "The rarity schema",
      "description": "Rarity of the ship",
      "type": "string",
      "default": "",
      "enum": [
        "common",
        "uncommon",
        "rare",
        "epic"
      ],
      "examples": [
        "common"
      ]
    },
    "requirements": {
      "$id": "#/properties/requirements",
      "title": "The requirements schema",
      "description": "List of all requirements needed before the ship can be build.",
      "type": "array",
      "additionalItems": true,
      "default": [],
      "items": {
        "$id": "#/properties/requirements/items",
        "anyOf": [
          {
            "$id": "#/properties/requirements/items/anyOf/0",
            "title": "The first anyOf schema",
            "description": "Instance of a requirement.",
            "required": [
              "name",
              "level"
            ],
            "type": "object",
            "additionalProperties": true,
            "default": {},
            "properties": {
              "name": {
                "$id": "#/properties/requirements/items/anyOf/0/properties/name",
                "title": "The name schema",
                "description": "The name of the required item.",
                "type": "string",
                "default": "",
                "examples": [
                  "shipyard"
                ]
              },
              "level": {
                "$id": "#/properties/requirements/items/anyOf/0/properties/level",
                "title": "The level schema",
                "description": "The level of the required item needed.",
                "type": "integer",
                "default": 0,
                "examples": [
                  28
                ]
              }
            }
          }
        ]
      }
    },
    "scrap_info": {
      "$id": "#/properties/scrap_info",
      "title": "The scrap_info schema",
      "description": "Information about scrapping the ship.",
      "required": [
        "scrapable"
      ],
      "type": "object",
      "additionalProperties": true,
      "default": {},
      "properties": {
        "required_scrapyard_level": {
          "$id": "#/properties/scrap_info/properties/required_scrapyard_level",
          "title": "The required_scrapyard_level schema",
          "description": "The scrapyard level needed to scrap the ship.",
          "type": "integer",
          "default": 0,
          "examples": [
            42
          ]
        },
        "scrapable": {
          "$id": "#/properties/scrap_info/properties/scrapable",
          "title": "The scrapable schema",
          "description": "Boolean wether the ship can be scrapped or not.",
          "type": "boolean",
          "default": true,
          "examples": [
            true
          ]
        }
      }
    },
    "ship_ability": {
      "$id": "#/properties/ship_ability",
      "title": "The ship_ability schema",
      "description": "Instance for the ability of the ship.",
      "required": [
        "title",
        "description",
        "percentage"
      ],
      "type": "object",
      "additionalProperties": true,
      "default": {},
      "properties": {
        "title": {
          "$id": "#/properties/ship_ability/properties/title",
          "title": "The title schema",
          "description": "Title of the ability.",
          "type": "string",
          "default": "",
          "examples": [
            "Protector"
          ]
        },
        "description": {
          "$id": "#/properties/ship_ability/properties/description",
          "title": "The description schema",
          "description": "Full explanation of the ability.",
          "type": "string",
          "default": "",
          "examples": [
            "When the Station..."
          ]
        },
        "percentage": {
          "$id": "#/properties/ship_ability/properties/percentage",
          "title": "The percentage schema",
          "description": "Boolean wether the ability is a percentage or not.",
          "type": "boolean",
          "default": true,
          "examples": [
            true
          ]
        }
      }
    },
    "ship_class": {
      "$id": "#/properties/ship_class",
      "title": "The ship_class schema",
      "description": "The class instance of the ship",
      "type": "string",
      "default": "",
      "enum": [
        "explorer",
        "interceptor",
        "battleship",
        "survey"
      ],
      "examples": [
        "interceptor"
      ]
    },
    "ship_name": {
      "$id": "#/properties/ship_name",
      "title": "The ship_name schema",
      "description": "Name of the ship.",
      "type": "string",
      "default": "",
      "examples": [
        "USS Saladin"
      ]
    },
    "tiers": {
      "$id": "#/properties/tiers",
      "title": "The tiers schema",
      "description": "List of all ship tiers.",
      "type": "array",
      "additionalItems": true,
      "default": [],
      "items": {
        "$id": "#/properties/tiers/items",
        "anyOf": [
          {
            "$id": "#/properties/tiers/items/anyOf/0",
            "title": "The first anyOf schema",
            "description": "Tier schema",
            "required": [
              "tier",
              "build_time",
              "repair_time",
              "components",
              "levels"
            ],
            "type": "object",
            "additionalProperties": true,
            "default": {},
            "properties": {
              "build_time": {
                "$id": "#/properties/tiers/items/anyOf/0/properties/build_time",
                "title": "The build_time schema",
                "description": "The total build time for the current tier (in seconds)",
                "type": "integer",
                "default": 0,
                "examples": [
                  1468800
                ]
              },
              "components": {
                "$id": "#/properties/tiers/items/anyOf/0/properties/components",
                "title": "The components schema",
                "description": "List of all components of the current tier.",
                "type": "array",
                "additionalItems": true,
                "default": [],
                "items": {
                  "$id": "#/properties/tiers/items/anyOf/0/properties/components/items",
                  "anyOf": [
                    {
                      "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0",
                      "title": "The first anyOf schema",
                      "description": "Single component schema",
                      "required": [
                        "name",
                        "img_url",
                        "build_cost",
                        "repair_cost",
                        "additional_info",
                        "quantity"
                      ],
                      "type": "object",
                      "additionalProperties": true,
                      "default": {},
                      "properties": {
                        "name": {
                          "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/name",
                          "title": "The name schema",
                          "description": "Name of the component.",
                          "type": "string",
                          "default": "",
                          "examples": [
                            "Phaser Bank"
                          ]
                        },
                        "additional_info": {
                          "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/additional_info",
                          "title": "The additional_info schema",
                          "description": "Instance of component specific info. Given sub schemas are optional, depending on component.",
                          "required": [],
                          "type": "object",
                          "additionalProperties": true,
                          "default": {},
                          "properties": {
                            "cargo_info": {
                              "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/additional_info/properties/cargo_info",
                              "title": "The cargo_info schema",
                              "description": "Info about the ships cargo capabilities.",
                              "required": [
                                "increased_cargo",
                                "increased_protected_cargo"
                              ],
                              "type": "object",
                              "additionalProperties": true,
                              "default": {},
                              "properties": {
                                "increased_cargo": {
                                  "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/additional_info/properties/cargo_info/properties/increased_cargo",
                                  "title": "The increased_cargo schema",
                                  "description": "Total amount of cargo the ship can hold.",
                                  "type": "integer",
                                  "default": 0
                                },
                                "increased_protected_cargo": {
                                  "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/additional_info/properties/cargo_info/properties/increased_protected_cargo",
                                  "title": "The increased_protected_cargo schema",
                                  "description": "The amount of cargo that is protected.",
                                  "type": "integer",
                                  "default": 0
                                }
                              }
                            },
                            "firing_pattern": {
                              "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/additional_info/properties/firing_pattern",
                              "title": "The firing_pattern schema",
                              "description": "Info about the weapon firing pattern.",
                              "required": [
                                "weapon_type",
                                "pattern"
                              ],
                              "type": "object",
                              "additionalProperties": true,
                              "default": {},
                              "properties": {
                                "pattern": {
                                  "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/additional_info/properties/firing_pattern/properties/pattern",
                                  "title": "The pattern schema",
                                  "description": "Array of the pattern on which the weapon fires.",
                                  "type": "array",
                                  "additionalItems": true,
                                  "default": [],
                                  "items": {
                                    "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/additional_info/properties/firing_pattern/properties/pattern/items",
                                    "anyOf": [
                                      {
                                        "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/additional_info/properties/firing_pattern/properties/pattern/items/anyOf/0",
                                        "title": "The first anyOf schema",
                                        "description": "Pattern item. 1 for firing, 0 for loading.",
                                        "type": "integer",
                                        "default": 1
                                      }
                                    ]
                                  }
                                },
                                "weapon_type": {
                                  "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/additional_info/properties/firing_pattern/properties/weapon_type",
                                  "title": "The weapon_type schema",
                                  "description": "Type of weapon.",
                                  "type": "string",
                                  "default": "",
                                  "enum": [
                                    "kinetic",
                                    "energy"
                                  ],
                                  "examples": [
                                    "kinetic"
                                  ]
                                }
                              }
                            },
                            "warp_info": {
                              "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/additional_info/properties/warp_info",
                              "title": "The warp_info schema",
                              "description": "Information about the warp capabilities.",
                              "required": [
                                "impulse_speed",
                                "warp_speed",
                                "warp_distance"
                              ],
                              "type": "object",
                              "additionalProperties": true,
                              "default": {},
                              "properties": {
                                "impulse_speed": {
                                  "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/additional_info/properties/warp_info/properties/impulse_speed",
                                  "title": "The impulse_speed schema",
                                  "description": "Value of the impulse speed at current tier.",
                                  "type": "integer",
                                  "default": 0
                                },
                                "warp_distance": {
                                  "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/additional_info/properties/warp_info/properties/warp_distance",
                                  "title": "The warp_distance schema",
                                  "description": "Warp distance at current tier.",
                                  "type": "integer",
                                  "default": 0
                                },
                                "warp_speed": {
                                  "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/additional_info/properties/warp_info/properties/warp_speed",
                                  "title": "The warp_speed schema",
                                  "description": "Value of warp speed at current tier.",
                                  "type": "number",
                                  "default": 0.0
                                }
                              }
                            }
                          }
                        },
                        "build_cost": {
                          "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost",
                          "title": "The build_cost schema",
                          "description": "All cost needed to upgrade the component",
                          "required": [],
                          "type": "object",
                          "additionalProperties": false,
                          "default": {},
                          "properties": {
                            "materials": {
                              "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost/properties/materials",
                              "title": "The materials schema",
                              "description": "All materials needed to upgrade the component.",
                              "type": "array",
                              "additionalItems": true,
                              "default": [],
                              "items": {
                                "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost/properties/materials/items",
                                "anyOf": [
                                  {
                                    "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost/properties/materials/items/anyOf/0",
                                    "title": "The first anyOf schema",
                                    "description": "Single material to upgrade the component.",
                                    "required": [
                                      "type",
                                      "rarity",
                                      "grade",
                                      "value"
                                    ],
                                    "type": "object",
                                    "additionalProperties": true,
                                    "default": {},
                                    "properties": {
                                      "type": {
                                        "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost/properties/materials/items/anyOf/0/properties/type",
                                        "title": "The type schema",
                                        "description": "The type of material needed.",
                                        "type": "string",
                                        "default": "",
                                        "enum": [
                                          "gas",
                                          "ore",
                                          "crystal"
                                        ],
                                        "examples": [
                                          "gas"
                                        ]
                                      },
                                      "grade": {
                                        "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost/properties/materials/items/anyOf/0/properties/grade",
                                        "title": "The grade schema",
                                        "description": "The grade in stars of the material.",
                                        "type": "integer",
                                        "default": 1,
                                        "enum": [
                                          1,
                                          2,
                                          3,
                                          4,
                                          5
                                        ],
                                        "examples": [
                                          1
                                        ]
                                      },
                                      "rarity": {
                                        "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost/properties/materials/items/anyOf/0/properties/rarity",
                                        "title": "The rarity schema",
                                        "description": "The rarity of the material.",
                                        "type": "string",
                                        "default": "",
                                        "enum": [
                                          "common",
                                          "uncommon",
                                          "rare",
                                          "epic"
                                        ],
                                        "examples": [
                                          "common"
                                        ]
                                      },
                                      "value": {
                                        "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost/properties/materials/items/anyOf/0/properties/value",
                                        "title": "The value schema",
                                        "description": "The value needed of the material.",
                                        "type": "integer",
                                        "default": 0,
                                        "examples": [
                                          275
                                        ]
                                      }
                                    }
                                  }
                                ]
                              }
                            },
                            "others": {
                              "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost/properties/others",
                              "title": "The others schema",
                              "description": "All other costs required to upgrade the component.",
                              "type": "array",
                              "additionalItems": true,
                              "default": [],
                              "items": {
                                "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost/properties/others/items",
                                "anyOf": [
                                  {
                                    "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost/properties/others/items/anyOf/0",
                                    "title": "The first anyOf schema",
                                    "description": "Single other cost instance.",
                                    "required": [
                                      "type",
                                      "value"
                                    ],
                                    "type": "object",
                                    "additionalProperties": true,
                                    "default": {},
                                    "properties": {
                                      "type": {
                                        "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost/properties/others/items/anyOf/0/properties/type",
                                        "title": "The type schema",
                                        "description": "The type of cost required.",
                                        "type": "string",
                                        "default": "",
                                        "examples": [
                                          "charged nanoprobes"
                                        ]
                                      },
                                      "value": {
                                        "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost/properties/others/items/anyOf/0/properties/value",
                                        "title": "The value schema",
                                        "description": "The amount required of this cost.",
                                        "type": "integer",
                                        "default": 0,
                                        "examples": [
                                          500
                                        ]
                                      }
                                    }
                                  }
                                ]
                              }
                            },
                            "resources": {
                              "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost/properties/resources",
                              "title": "The resources schema",
                              "description": "All resources needed to upgrade the component.",
                              "type": "array",
                              "additionalItems": true,
                              "default": [],
                              "items": {
                                "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost/properties/resources/items",
                                "anyOf": [
                                  {
                                    "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost/properties/resources/items/anyOf/0",
                                    "title": "The first anyOf schema",
                                    "description": "Single resource to upgrade the component.",
                                    "required": [
                                      "type",
                                      "value"
                                    ],
                                    "type": "object",
                                    "additionalProperties": true,
                                    "default": {},
                                    "properties": {
                                      "type": {
                                        "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost/properties/resources/items/anyOf/0/properties/type",
                                        "title": "The type schema",
                                        "description": "Type of resource needed.",
                                        "type": "string",
                                        "default": "",
                                        "enum": [
                                          "parsteel",
                                          "tritanium",
                                          "dilithium"
                                        ],
                                        "examples": [
                                          "tritanium"
                                        ]
                                      },
                                      "value": {
                                        "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/build_cost/properties/resources/items/anyOf/0/properties/value",
                                        "title": "The value schema",
                                        "description": "The amount needed of the current resource.",
                                        "type": "integer",
                                        "default": 0,
                                        "examples": [
                                          1300000
                                        ]
                                      }
                                    }
                                  }
                                ]
                              }
                            }
                          }
                        },
                        "img_url": {
                          "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/img_url",
                          "title": "The img_url schema",
                          "description": "An URL of the component image.",
                          "type": "string",
                          "default": "",
                          "examples": [
                            "url"
                          ]
                        },
                        "quantity": {
                          "$id": "#/properties/tiers/items/anyOf/0/properties/components/items/anyOf/0/properties/quantity",
                          "title": "The quantity schema",
                          "description": "Quantity of the component.",
                          "type": "integer",
