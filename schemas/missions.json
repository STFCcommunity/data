{
  "$id": "http://example.com/example.json",
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "The root schema",
  "description": "A schema for missions in STFC.",
  "type": "array",
  "additionalItems": true,
  "default": [],
  "examples": [
    [
      {
        "faction": "federation",
        "location": {
          "$ref": "reference to system json",
          "planet": "Andoria prime"
        },
        "mission_title": "Beneath the Ice",
        "mission_type": "start",
        "next_mission": {
          "$ref": "reference to mission."
        },
        "objectives": [
          {
            "title": "Meet the Romulan Informant on Moya.",
            "main_objective": {
              "$ref": "#objectives/items"
            },
            "obj_id": 1,
            "task": [
              {
                "$ref": "reference to system json",
                "planet": "Moya",
                "task_type": "travel"
              },
              {
                "$ref": "reference to system json",
                "task_title": "Destroy Suliban Interceptor",
                "task_type": "destroy"
              },
              {
                "$ref": "reference to system json",
                "task_title": "Kill Suliban Interceptor",
                "task_type": "kill"
              },
              {
                "resource": "dilithium",
                "task_type": "deliver",
                "value": 300
              },
              {
                "options": [
                  {
                    "option_id": 1,
                    "option_title": "smuggle Garan across...",
                    "rewards": {
                      "$ref": "reference"
                    }
                  }
                ],
                "task_type": "choose"
              },
              {
                "task_title": "Return to your station",
                "task_type": "return"
              }
            ]
          }
        ],
        "previous_mission": {
          "$ref": "reference to mission."
        },
        "recommended_ops": 27,
        "rewards": [
          {
            "materials": [
              {
                "type": "gas",
                "grade": 3,
                "rarity": "uncommon",
                "value": 50
              }
            ],
            "others": [
              {
                "type": "federation credits",
                "value": 330
              }
            ],
            "resources": [
              {
                "type": "parsteel",
                "value": 1
              }
            ],
            "reward_id": 1
          }
        ],
        "storyline": "The emerging Aenar on Andoria..."
      }
    ]
  ],
  "items": {
    "$id": "#/items",
    "anyOf": [
      {
        "$id": "#/items/anyOf/0",
        "title": "The first anyOf schema",
        "description": "Single mission instance.",
        "required": [
          "mission_title",
          "storyline",
          "faction",
          "mission_type",
          "location",
          "objectives",
          "rewards"
        ],
        "type": "object",
        "additionalProperties": true,
        "default": {},
        "properties": {
          "faction": {
            "$id": "#/items/anyOf/0/properties/faction",
            "title": "The faction schema",
            "description": "To which faction the missions belongs to.",
            "type": "string",
            "default": "",
            "enum": [
              "neutral",
              "federation",
              "romulan",
              "klingon",
              "augment",
              "rogue"
            ],
            "examples": [
              "federation"
            ]
          },
          "location": {
            "$id": "#/items/anyOf/0/properties/location",
            "title": "The location schema",
            "description": "Location of the mission.",
            "required": [
              "$ref"
            ],
            "type": "object",
            "additionalProperties": true,
            "default": {},
            "properties": {
              "$ref": {
                "$id": "#/items/anyOf/0/properties/location/properties/%24ref",
                "title": "The $ref schema",
                "description": "Reference to the system file.",
                "type": "string",
                "default": "",
                "examples": [
                  "reference to system json"
                ]
              },
              "planet": {
                "$id": "#/items/anyOf/0/properties/location/properties/planet",
                "title": "The planet schema",
                "description": "(Optional) Planet where the mission is.",
                "type": "string",
                "default": "",
                "examples": [
                  "Andoria prime"
                ]
              }
            }
          },
          "mission_title": {
            "$id": "#/items/anyOf/0/properties/mission_title",
            "title": "The mission_title schema",
            "description": "Title of the missions.",
            "type": "string",
            "default": "",
            "examples": [
              "Beneath the Ice"
            ]
          },
          "mission_type": {
            "$id": "#/items/anyOf/0/properties/mission_type",
            "title": "The mission_type schema",
            "description": "The type of mission.",
            "type": "string",
            "default": "",
            "enum": [
              "start",
              "auto link / continuation"
            ],
            "examples": [
              "start"
            ]
          },
          "next_mission": {
            "$id": "#/items/anyOf/0/properties/next_mission",
            "title": "The next_mission schema",
            "description": "(Optional) Reference to the next mission in this chain, if any.",
            "required": [
              "$ref"
            ],
            "type": "object",
            "additionalProperties": true,
            "default": {},
            "properties": {
              "$ref": {
                "$id": "#/items/anyOf/0/properties/next_mission/properties/%24ref",
                "title": "The $ref schema",
                "description": "Reference to the next mission file.",
                "type": "string",
                "default": "",
                "examples": [
                  "reference to mission."
                ]
              }
            }
          },
          "objectives": {
            "$id": "#/items/anyOf/0/properties/objectives",
            "title": "The objectives schema",
            "description": "List of all objectives in this mission.",
            "type": "array",
            "additionalItems": true,
            "default": [],
            "items": {
              "$id": "#/items/anyOf/0/properties/objectives/items",
              "anyOf": [
                {
                  "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0",
                  "title": "The first anyOf schema",
                  "description": "Single mission objective instance.",
                  "required": [
                    "obj_id",
                    "title",
                    "task"
                  ],
                  "type": "object",
                  "additionalProperties": true,
                  "default": {},
                  "properties": {
                    "title": {
                      "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/title",
                      "title": "The title schema",
                      "description": "Title of the objective.",
                      "type": "string",
                      "default": "",
                      "examples": [
                        "Meet the Romulan Informant on Moya."
                      ]
                    },
                    "main_objective": {
                      "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/main_objective",
                      "title": "The main_objective schema",
                      "description": "(Optional) Holds a reference to the main objective when this is a child objective based on a 'choose' type objective.",
                      "required": [
                        "$ref"
                      ],
                      "type": "object",
                      "additionalProperties": true,
                      "default": {},
                      "properties": {
                        "$ref": {
                          "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/main_objective/properties/%24ref",
                          "title": "The $ref schema",
                          "description": "Reference to the main objective.",
                          "type": "string",
                          "default": "",
                          "examples": [
                            "reference"
                          ]
                        }
                      }
                    },
                    "obj_id": {
                      "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/obj_id",
                      "title": "The obj_id schema",
                      "description": "Numeric id of the objective to identify the objectives flow.",
                      "type": "integer",
                      "default": 0,
                      "examples": [
                        1
                      ]
                    },
                    "task": {
                      "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task",
                      "title": "The task schema",
                      "description": "List of all types of tasks. The objective can have one of these, based on task type.",
                      "type": "array",
                      "additionalItems": true,
                      "default": [],
                      "items": {
                        "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items",
                        "anyOf": [
                          {
                            "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/0",
                            "title": "The first anyOf schema",
                            "description": "Task for the 'travel' mission type.",
                            "required": [
                              "task_type"
                            ],
                            "type": "object",
                            "additionalProperties": true,
                            "default": {},
                            "properties": {
                              "$ref": {
                                "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/0/properties/%24ref",
                                "title": "The $ref schema",
                                "description": "(Optional) A destination reference to the systems file. When not implemented, the destination is the player' home system.",
                                "type": "string",
                                "default": "",
                                "examples": [
                                  "reference to system json"
                                ]
                              },
                              "planet": {
                                "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/0/properties/planet",
                                "title": "The planet schema",
                                "description": "(Optional) Planet where the destination is.",
                                "type": "string",
                                "default": "",
                                "examples": [
                                  "Moya"
                                ]
                              },
                              "task_type": {
                                "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/0/properties/task_type",
                                "title": "The task_type schema",
                                "description": "The type of task.",
                                "type": "string",
                                "const": "travel",
                                "default": "travel"
                              }
                            }
                          },
                          {
                            "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/1",
                            "title": "The second anyOf schema",
                            "description": "Task for the 'destroy' mission type.",
                            "required": [
                              "task_type",
                              "task_title"
                            ],
                            "type": "object",
                            "additionalProperties": true,
                            "default": {},
                            "properties": {
                              "$ref": {
                                "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/1/properties/%24ref",
                                "title": "The $ref schema",
                                "description": "(Optional) A reference to the systems file where the target is. When not implemented, the destination is the player' home system.",
                                "type": "string",
                                "default": "",
                                "examples": [
                                  "reference to system json"
                                ]
                              },
                              "task_title": {
                                "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/1/properties/task_title",
                                "title": "The task_title schema",
                                "description": "The title of the task.",
                                "type": "string",
                                "default": "",
                                "examples": [
                                  "Destroy Suliban Interceptor"
                                ]
                              },
                              "task_type": {
                                "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/1/properties/task_type",
                                "title": "The task_type schema",
                                "description": "The type of task.",
                                "type": "string",
                                "const": "destroy",
                                "default": "destroy"
                              }
                            }
                          },
                          {
                            "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/2",
                            "title": "The third anyOf schema",
                            "description": "Task for the 'kill' mission type.",
                            "required": [
                              "task_type",
                              "task_title"
                            ],
                            "type": "object",
                            "additionalProperties": true,
                            "default": {},
                            "properties": {
                              "$ref": {
                                "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/2/properties/%24ref",
                                "title": "The $ref schema",
                                "description": "(Optional) A reference to the systems file where the target is. When not implemented, the destination is the player' home system.",
                                "type": "string",
                                "default": "",
                                "examples": [
                                  "reference to system json"
                                ]
                              },
                              "task_title": {
                                "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/2/properties/task_title",
                                "title": "The task_title schema",
                                "description": "The title of the task.",
                                "type": "string",
                                "default": "",
                                "examples": [
                                  "Kill Suliban Interceptor"
                                ]
                              },
                              "task_type": {
                                "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/2/properties/task_type",
                                "title": "The task_type schema",
                                "description": "The type of task.",
                                "type": "string",
                                "const": "kill",
                                "default": "kill"
                              }
                            }
                          },
                          {
                            "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/3",
                            "title": "The fourth anyOf schema",
                            "description": "Task for the 'deliver' mission type.",
                            "required": [
                              "task_type",
                              "resource",
                              "value"
                            ],
                            "type": "object",
                            "additionalProperties": true,
                            "default": {},
                            "properties": {
                              "resource": {
                                "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/3/properties/resource",
                                "title": "The resource schema",
                                "description": "The type of resource used.",
                                "type": "string",
                                "default": "",
                                "examples": [
                                  "dilithium"
                                ]
                              },
                              "task_type": {
                                "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/3/properties/task_type",
                                "title": "The task_type schema",
                                "description": "The type of task.",
                                "type": "string",
                                "const": "deliver",
                                "default": "deliver"
                              },
                              "value": {
                                "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/3/properties/value",
                                "title": "The value schema",
                                "description": "The amount of this resource required.",
                                "type": "integer",
                                "default": 0,
                                "examples": [
                                  300
                                ]
                              }
                            }
                          },
                          {
                            "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/4",
                            "title": "The fifth anyOf schema",
                            "description": "Task for the 'choose' mission type.",
                            "required": [
                              "task_type",
                              "options"
                            ],
                            "type": "object",
                            "additionalProperties": true,
                            "default": {},
                            "properties": {
                              "options": {
                                "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/4/properties/options",
                                "title": "The options schema",
                                "description": "List of all options to choose from.",
                                "type": "array",
                                "additionalItems": true,
                                "default": [],
                                "items": {
                                  "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/4/properties/options/items",
                                  "anyOf": [
                                    {
                                      "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/4/properties/options/items/anyOf/0",
                                      "title": "The first anyOf schema",
                                      "description": "Single option instance.",
                                      "required": [
                                        "option_id",
                                        "option_title",
                                        "rewards"
                                      ],
                                      "type": "object",
                                      "additionalProperties": true,
                                      "default": {},
                                      "properties": {
                                        "option_id": {
                                          "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/4/properties/options/items/anyOf/0/properties/option_id",
                                          "title": "The option_id schema",
                                          "description": "The id of the option, used to sort the options.",
                                          "type": "integer",
                                          "default": 0,
                                          "examples": [
                                            1
                                          ]
                                        },
                                        "option_title": {
                                          "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/4/properties/options/items/anyOf/0/properties/option_title",
                                          "title": "The option_title schema",
                                          "description": "Title of the option.",
                                          "type": "string",
                                          "default": "",
                                          "examples": [
                                            "smuggle Garan across..."
                                          ]
                                        },
                                        "rewards": {
                                          "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/4/properties/options/items/anyOf/0/properties/rewards",
                                          "title": "The rewards schema",
                                          "description": "Reference to the rewards able to gain by choosing this option.",
                                          "required": [
                                            "$ref"
                                          ],
                                          "type": "object",
                                          "additionalProperties": true,
                                          "default": {},
                                          "properties": {
                                            "$ref": {
                                              "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/4/properties/options/items/anyOf/0/properties/rewards/properties/%24ref",
                                              "title": "The $ref schema",
                                              "description": "Reference to the rewards property by reward_id.",
                                              "type": "string",
                                              "default": "",
                                              "examples": [
                                                "reference"
                                              ]
                                            }
                                          }
                                        }
                                      }
                                    }
                                  ]
                                },
                                "minItems": 2
                              },
                              "task_type": {
                                "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/4/properties/task_type",
                                "title": "The task_type schema",
                                "description": "The type of task.",
                                "type": "string",
                                "const": "choose",
                                "default": "choose"
                              }
                            }
                          },
                          {
                            "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/5",
                            "title": "The sixth anyOf schema",
                            "description": "Task for the 'return' mission type.",
                            "required": [
                              "task_type",
                              "task_title"
                            ],
                            "type": "object",
                            "additionalProperties": true,
                            "default": {},
                            "properties": {
                              "task_title": {
                                "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/5/properties/task_title",
                                "title": "The task_title schema",
                                "description": "Title of the task.",
                                "type": "string",
                                "const": "Return to your station",
                                "default": "Return to your station"
                              },
                              "task_type": {
                                "$id": "#/items/anyOf/0/properties/objectives/items/anyOf/0/properties/task/items/anyOf/5/properties/task_type",
                                "title": "The task_type schema",
                                "description": "The type of task.",
                                "type": "string",
                                "const": "return",
                                "default": "return"
                              }
                            }
                          }
                        ]
                      },
                      "maxItems": 1
                    }
                  }
                }
              ]
            }
          },
          "previous_mission": {
            "$id": "#/items/anyOf/0/properties/previous_mission",
            "title": "The previous_mission schema",
            "description": "(Optional) Reference to the previous mission in this chain, if any.",
            "required": [
              "$ref"
            ],
            "type": "object",
            "additionalProperties": true,
            "default": {},
            "properties": {
              "$ref": {
                "$id": "#/items/anyOf/0/properties/previous_mission/properties/%24ref",
                "title": "The $ref schema",
                "description": "Reference to the previous mission file.",
                "type": "string",
                "default": "",
                "examples": [
                  "reference to mission."
                ]
              }
            }
          },
          "recommended_ops": {
            "$id": "#/items/anyOf/0/properties/recommended_ops",
            "title": "The recommended_ops schema",
            "description": "(Optional) Operations level recommended to complete this mission.",
            "type": "integer",
            "default": 0,
            "examples": [
              27
            ]
          },
          "rewards": {
            "$id": "#/items/anyOf/0/properties/rewards",
            "title": "The rewards schema",
            "description": "List of all rewards for this mission. When the mission has no 'choose' type tasks, only one list item is used to describe all rewards. Otherwhise all tasks are split by an id.",
            "type": "array",
            "additionalItems": true,
            "default": [],
            "items": {
              "$id": "#/items/anyOf/0/properties/rewards/items",
              "anyOf": [
                {
                  "$id": "#/items/anyOf/0/properties/rewards/items/anyOf/0",
                  "title": "The first anyOf schema",
                  "description": "Single reward list instance.",
                  "required": [
                    "resources",
                    "materials",
                    "others"
                  ],
                  "type": "object",
                  "additionalProperties": false,
                  "default": {},
                  "properties": {
                    "materials": {
                      "$id": "#/items/anyOf/0/properties/rewards/items/anyOf/0/properties/materials",
                      "title": "The materials schema",
                      "description": "List of all materials gained by completing the mission.",
                      "type": "array",
                      "additionalItems": true,
                      "default": [],
                      "items": {
                        "$id": "#/items/anyOf/0/properties/rewards/items/anyOf/0/properties/materials/items",
                        "anyOf": [
                          {
                            "$id": "#/items/anyOf/0/properties/rewards/items/anyOf/0/properties/materials/items/anyOf/0",
                            "title": "The first anyOf schema",
                            "description": "Single material reward instance.",
                            "required": [
                              "type",
                              "rarity",
                              "grade",
                              "value"
                            ],
                            "type": "object",
                            "additionalProperties": true,
                            "default": {},
                            "properties": {
                              "type": {
                                "$id": "#/items/anyOf/0/properties/rewards/items/anyOf/0/properties/materials/items/anyOf/0/properties/type",
                                "title": "The type schema",
                                "description": "Type of material gained.",
                                "type": "string",
                                "default": "",
                                "enum": [
                                  "gas",
                                  "ore",
                                  "crystal"
                                ],
                                "examples": [
                                  "gas"
                                ]
                              },
                              "grade": {
                                "$id": "#/items/anyOf/0/properties/rewards/items/anyOf/0/properties/materials/items/anyOf/0/properties/grade",
                                "title": "The grade schema",
                                "description": "The grade in stars of the material.",
                                "type": "integer",
                                "default": 0,
                                "enum": [
                                  1,
                                  2,
                                  3,
                                  4,
                                  5
                                ],
                                "examples": [
                                  3
                                ]
                              },
                              "rarity": {
                                "$id": "#/items/anyOf/0/properties/rewards/items/anyOf/0/properties/materials/items/anyOf/0/properties/rarity",
                                "title": "The rarity schema",
                                "description": "Rarity of the material",
                                "type": "string",
                                "default": "",
                                "enum": [
                                  "common",
                                  "uncommon",
                                  "rare",
                                  "epic"
                                ],
                                "examples": [
                                  "uncommon"
                                ]
                              },
                              "value": {
                                "$id": "#/items/anyOf/0/properties/rewards/items/anyOf/0/properties/materials/items/anyOf/0/properties/value",
                                "title": "The value schema",
                                "description": "The amount of this material gained.",
                                "type": "integer",
                                "default": 0,
                                "examples": [
                                  50
                                ]
                              }
                            }
                          }
                        ]
                      }
                    },
                    "others": {
                      "$id": "#/items/anyOf/0/properties/rewards/items/anyOf/0/properties/others",
                      "title": "The others schema",
                      "description": "List of all other rewards gained.",
                      "type": "array",
                      "additionalItems": true,
